    "version": "0.2.0",
    "configurations": [
        {
            "name": "Q2RTX +map first",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/q2rtx",
            "args": [
                "+set", "developer", "1",
                "+cheats", "1",
                "+map", "first",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
                // {
                //     "description": "Enable pretty-printing for gdb",
